# Google AI API Key (required for README generation)
GOOGLE_API_KEY=your_google_api_key_here

# GitHub OAuth Configuration (required for repositories and history features)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
# IMPORTANT: Set this to match your GitHub OAuth app's callback URL
GITHUB_REDIRECT_URI=https://autodocai.vercel.app/auth/callback

# GitHub Database Configuration (for history storage - optional)
GITHUB_DATA_REPO_OWNER=your_github_username
GITHUB_DATA_REPO_NAME=autodoc-ai-data
GITHUB_DATA_TOKEN=your_personal_access_token
